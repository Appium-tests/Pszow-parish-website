<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Photo gallery page">

    <listeners>
        <listener class-name="qa.listener.TestListener"/>
    </listeners>

    <test name="Expanding the photo gallery contents">
        <classes>
            <class name="photogallery.PhotoGalleryPageTest">
                <methods>
                    <include name="expandingGallery"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="collapsing the photo gallery contents">
        <classes>
            <class name="photogallery.PhotoGalleryPageTest">
                <methods>
                    <include name="collapsingGallery"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>