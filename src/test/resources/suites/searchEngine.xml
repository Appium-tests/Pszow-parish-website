<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Search engine">

    <listeners>
        <listener class-name="qa.listener.TestListener"/>
    </listeners>
    
    <test name="Input text verification">
        <classes>
            <class name="searchengine.InputTextVerificationTest"/>
        </classes>
    </test>

    <test name="Searching articles">
        <parameter name="filename" value="PP_Phrases"/>

        <classes>
            <class name="searchengine.SearchingArticlesTest"/>
        </classes>
    </test>

</suite>